#安装
    npm i
    sudo apt-get update
    sudo apt-get install redis-server

    #redis 安装指定版本
    apt-get remove redis-server
    apt-get autoremove

    # 命令用法如下：
    wget http://download.redis.io/releases/redis-4.0.6.tar.gz
    sudo tar -zxvf redis-4.0.6.tar.gz
    /usr/local/redis/src$ cd redis-4.0.1/
    /usr/local/redis/redis-4.0.1/src$ sudo make

#启动方式
    # 启动redis
    redis-server

    # 检查redis 版本
    redis-cli --version

    # 检查redis 是否成功
    redis-cli
    redis 127.0.0.1:6379> ping
    PONG

    #npm 启动
    npm run dev